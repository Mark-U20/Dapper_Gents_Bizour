{
  "name": "biz-our_bizarre_bazaar",
  "version": "1.2.3",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "dev:server": "cd ./server && nodemon server.js",
    "dev:client": "cd ./client && npm start",
    "dev": "concurrently  \"npm run dev:server\"  \"npm run dev:client\"",
    "start": "cd ./server && npm start",
    "build:client": "cd ./client && npm run build",
    "build": "npm run seed && npm run build:client",
    "seed": "cd ./server/seeders && node seed.js",
    "format": "prettier --write \"client/**/*.{js,jsx}\" && prettier --write \"server/**/*.{js,jsx}\"",
    "install:server": "cd ./server && npm i",
    "install:client": "cd ./client && npm i",
    "install:dep": "npm run install:server && npm run install:client",
    "install:main": "npm i",
    "install": "npm run install:dep && npm run install:main"
  },
  "keywords": [],
  "author": "dapper_gents",
  "license": "GPL-3.0",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "react-router-dom": "^6.3.0"
  }
}
